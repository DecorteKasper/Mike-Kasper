{
  "name": "mike-kasper",
  "version": "0.0.0",
  "scripts": {
    "dev": "npx lerna run --scope={lifeguard,api} dev",
    "dev:api": "npx lerna run --scope=api dev",
    "dev:pwa": "npx lerna run --scope=lifeguard dev",
    "translate:pwa": "npx run generate:translations -w lifeguard",
    "build:pwa": "npx lerna run --scope=lifeguard build",
    "build:api": "npx lerna run --scope=api build"
  },
  "keywords": [],
  "author": "Mike & Kasper",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "lucide-vue-next": "^0.279.0",
    "react": "^18.2.0",
    "vue-multiselect": "^3.0.0-beta.3"
  },
  "devDependencies": {
    "lerna": "^8.0.0"
  }
}
